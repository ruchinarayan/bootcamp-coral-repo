<h1>Theaters#index</h1>
<h1>mfshamim</h1>

<table>
	<tr>
		<td>Theater Name</td>
		<td>Theater Address</td>
		<td>Theater Phone Number</td>
		<td>Links</td>
	</tr>
<% @theaters.each do |theater|%>
	<tr>
		<td><%= theater.theater_name %></td>
		<td><%= theater.theater_address %></td>
		<td><%= theater.theater_phone %></td>
		<td>[<%= link_to "Show", theater_path(theater) %>,
			 <%= link_to "Edit", edit_theater_path(theater) %>,
			 <%= link_to "Delete", theater_path(theater), method: :delete %>]
		</td>
	</tr>
<% end %>
</table>

<p>
	<%= link_to "Create New Theater", new_theater_path %>
</p>

