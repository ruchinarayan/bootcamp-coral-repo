<h1>Create New Theater</h1>

<%= form_for(@theater) do |f| %>

<p>
    <%= f.label :theater_name %><br>
    <%= f.text_field :theater_name %>
</p>
<p>
    <%= f.label :theater_address %><br>
    <%= f.text_field :theater_address %>
</p>
<p>
    <%= f.label :theater_phone %><br>
    <%= f.text_field :theater_phone %>
</p>
<p>
	<%= f.submit %>
</p>

<% end %>
